{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "interface-name": false,
    "member-access": [true, "no-public"],
    "one-variable-per-declaration": false,
    "ordered-imports": {
      "options": {
        "import-sources-order": "any",
        "named-imports-order": "case-insensitive"
      }
    }
  }
}
